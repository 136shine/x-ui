{
  "name": "x-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "dev": "docz dev",
    "start": "npm run dev",
    "build:doc": "rimraf doc-site && docz build",
    "preview:doc": "docz serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ant-design/icons": "^4.1.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@umijs/fabric": "^2.0.8",
    "antd": "^4.2.0",
    "babel-plugin-import": "^1.13.0",
    "classnames": "^2.2.6",
    "commitizen": "^4.0.5",
    "cz-conventional-changelog": "^3.1.0",
    "docz": "^2.3.1",
    "gatsby-plugin-import": "^2.1.5",
    "gatsby-plugin-less": "^3.2.0",
    "less": "^3.11.1",
    "prettier": "^2.0.5",
    "prismjs": "^1.20.0",
    "raw-loader": "^4.0.1",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-simple-code-editor": "^0.11.0",
    "react-use": "^14.2.0",
    "rimraf": "^3.0.2"
  },
  "lint-staged": {
    "components/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "components/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  }
}
